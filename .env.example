# X.com 用户名或邮箱
X_USERNAME=your_x_username_or_email
# X.com 密码
X_PASSWORD=your_x_passworde

# Supabase 数据库配置
# 从 Supabase 项目设置中获取
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# 服务器配置
# 服务器运行端口
PORT=3001

# 服务器主机地址
HOST=localhost

# 运行环境 (development, production)
NODE_ENV=development

# 调度器配置
# Cron 表达式，控制数据收集频率
# 默认: '0 * * * *' (每小时执行一次)
# 示例: '*/5 * * * *' (每5分钟执行一次)
# 示例: '0 */6 * * *' (每6小时执行一次)
CRON_EXPRESSION=0 * * * *

# 时区设置
TIMEZONE=Asia/Shanghai

# 是否启用调度器 (true/false)
SCHEDULER_ENABLED=true
